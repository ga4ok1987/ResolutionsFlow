<Application x:Class="ResolutionsFlow.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
             xmlns:markup="clr-namespace:Wpf.Ui.Markup;assembly=Wpf.Ui"
             StartupUri="Presentation/Views/ShellWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>

                <!-- WPF UI dictionaries -->
                <markup:ControlsDictionary />
                <markup:ThemesDictionary Theme="Light" />

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
